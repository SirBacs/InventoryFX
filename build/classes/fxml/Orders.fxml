<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" prefHeight="655.0" prefWidth="862.0">
   <children>
      <HBox spacing="10" alignment="CENTER_LEFT" style="-fx-padding: 10;">
         <Label text="Search:" />
         <TextField fx:id="searchField" promptText="Search orders..." prefWidth="300.0" />
      </HBox>

      <TableView fx:id="ordersTable" prefHeight="531.0" prefWidth="862.0">
         <columns>
            <TableColumn fx:id="orderIdColumn" text="Order ID" prefWidth="100.0" />
            <TableColumn fx:id="productIdColumn" text="Product ID" prefWidth="100.0" />
            <TableColumn fx:id="quantityColumn" text="Quantity" prefWidth="100.0" />
            <TableColumn fx:id="orderDateColumn" text="Order Date" prefWidth="125.0" />
            <TableColumn fx:id="statusColumn" text="Status" prefWidth="100.0" />
            <TableColumn fx:id="customerInfoColumn" text="Customer Info" prefWidth="150.0" />
            <TableColumn fx:id="feedbackColumn" text="Feedback" prefWidth="150.0" />
         </columns>
      </TableView>
   </children>
</VBox>
