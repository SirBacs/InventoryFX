            </TextField>
            <Label text="Unit">
               <opaqueInsets>
                  <Insets bottom="5.0" top="5.0" />
               </opaqueInsets>
            </Label>
            <TextField fx:id="unitField">
               <VBox.margin>
                  <Insets bottom="15.0" />
               </VBox.margin>
            </TextField>
         </children>
         <opaqueInsets>
            <Insets bottom="5.0" top="5.0" />
         </opaqueInsets>
      </VBox>
      <Button layoutX="414.0" layoutY="336.0" mnemonicParsing="false" onAction="#handleSave" prefHeight="25.0" prefWidth="79.0" text="SAVE" />
      <Button layoutX="507.0" layoutY="336.0" mnemonicParsing="false" onAction="#handleCancel" prefHeight="25.0" prefWidth="79.0" text="CANCEL" />
   </children>
</AnchorPane>
