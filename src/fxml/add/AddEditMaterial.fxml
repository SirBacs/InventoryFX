      <VBox layoutX="35.0" layoutY="37.0" prefHeight="287.0" prefWidth="530.0">
         <children>
            <Label text="Material Name">
               <opaqueInsets>
                  <Insets bottom="5.0" top="5.0" />
               </opaqueInsets>
               <VBox.margin>
                  <Insets bottom="5.0" top="5.0" />
               </VBox.margin>
            </Label>
            <TextField fx:id="nameField">
               <VBox.margin>
                  <Insets bottom="15.0" />
               </VBox.margin>
            </TextField>
            <Label text="Description">
               <opaqueInsets>
                  <Insets bottom="5.0" top="5.0" />
               </opaqueInsets>
            </Label>
            <TextArea fx:id="descriptionField" prefHeight="83.0" prefWidth="574.0">
               <VBox.margin>
                  <Insets bottom="15.0" />
               </VBox.margin>
            </TextArea>
            <Label text="Quantity">
               <opaqueInsets>
                  <Insets bottom="5.0" top="5.0" />
               </opaqueInsets>
            </Label>
            <TextField fx:id="quantityField">
               <VBox.margin>
                  <Insets bottom="15.0" />
